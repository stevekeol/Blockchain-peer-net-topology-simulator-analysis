{
  "name": "Blockchain-peer-net-topology-simulator-analysis",
  "version": "0.1.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "stevekeol"
  },
  "description": "Peer net topology graph simulator & analysis",
  "keywords": [
    "blockchain",
    "peer",
    "net",
    "topology",
    "react",
    "typescript",
    "vite",
    "electron"
  ],
  "repository": {
    "url": "https://github.com/stevekeol/Blockchain-peer-net-topology-simulator-analysis"
  },
  "productName": "let me think more",
  "scripts": {
    "dev": "concurrently \"SET BROWSER=none && yarn dev:vite\" \" yarn dev:electron",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc"
  },
  "dependencies": {
    "@antv/g6": "^4.2.5",
    "electron-is-dev": "^1.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "concurrently": "^5.3.0",
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.0-beta.45",
    "autoprefixer": "^10.2.3",
    "postcss": "^8.2.4",
    "tailwindcss": "^2.0.2"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
